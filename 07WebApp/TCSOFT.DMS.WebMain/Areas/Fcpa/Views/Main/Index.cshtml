    <script>
            (function($){
                $.getUrlParam
                    = function(name)
                {
                    var reg
                        = new RegExp("(^|&)"+
                        name +"=([^&]*)(&|$)");
                    var r
                        = window.location.search.substr(1).match(reg);
                    if (r!=null) return unescape(r[2]); return null;
                }
            })(jQuery);
            $.fn.toJSON = function()
            {
                var o = {};
                var a = this.serializeArray();
                $.each(a, function() {
                    var name = this.name;
                    var value = this.value;
                    var paths = this.name.split(".");
                    var len = paths.length;
                    var obj = o;
                    $.each(paths,function(i,e){
                        if(i == len-1){
                            if (obj[e]) {
                                if (!obj[e].push) {
                                    obj[e] = [obj[e]];
                                }
                                obj[e].push(value || '');
                            } else {
                                obj[e] = value || '';
                            }
                        }else{
                            if(!obj[e]){
                                obj[e] = {};
                            }
                        }
                        obj = o[e];
                    });
                });
                return o;
            };
            $(function () {
                //手风琴
                var Accordion = function (el, multiple) {
                    this.el = el || {};
                    this.multiple = multiple || false;
                    // Variables privadas
                    var links = this.el.find('.link');
                    // Evento
                    links.on('click', {el: this.el, multiple: this.multiple}, this.dropdown)
                }

                Accordion.prototype.dropdown = function (e) {
                    var $el = e.data.el;
                    $this = $(this),
                        $next = $this.next();

                    $(".link").next().slideUp();
                    if ($next.is(":hidden")) {
                        $next.slideDown();
                    } else {
                        $next.slideUp();
                    }

                    if (!e.data.multiple) {
                        $el.find('.submenu').not($next).slideUp().parent().removeClass('open');
                    }
                    ;
                }
                var accordion = new Accordion($('.accordion'), true);

                // 初始化默认首页
                var $ss = $("#uloneli li div").next()
                $ss.slideDown();
                $(".submenu li").css("color", "#0033CC");
                $(".submenu li").css("background-color", "#0033CC");
                var $ww = $ss.find("li").first().find("a").first();
                $ww.parent().css("background-color", "#a7a8aa");
                $(".imgcd").hide();
                $ww.prev().show();
                $ww.removeClass("hover");
                var url = $ww.attr("value");
                $("#ContentArea").panel({href: url});
            });

            //选中变色
            function SelectedL(seleid) {
                $(".submenu li").css("color", "#0033CC");
                $(".submenu li").css("background-color", "#0033CC");
                $(".imgcd").hide();
                $("#" + seleid).prev().show();
                $("#" + seleid).parent().css("background-color", "#a7a8aa");
                $("#" + seleid).removeClass("hover");
            }
            //退出
            function Exit() {
                window.opener = null;
                window.open('', '_self');
                window.close();
            }

            function ShowContent(contenturl, seleid) {
                SelectedL(seleid);
                $("#ContentArea").panel({href: contenturl});
            }
